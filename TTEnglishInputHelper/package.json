{
  "name": "tteih-dev",
  "version": "0.4.0",
  "description": "English input helper for UTAU",
  "main": "build.js",
  "directories": {
    "test": "built/test"
  },
  "scripts": {
    "test": "node ./tool/prepareToTest.js && webpack --config webpack.config.test.js && mocha --opts ./src/test/mocha.opts ./built/test",
    "preparec": "node prepareToCompile.js",
    "compile": "node prepareToCompile.js && webpack",
    "exec": "electron built/app",
    "build": "node build.js",
    "reporters": "mocha --reporters",
    "ncu": "ncu",
    "ncu-u": "ncu -u",
    "ncu-a": "ncu -a",
    "update": "ncu -u && npm update"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hunyosi/tteih.git"
  },
  "keywords": [
    "UTAU",
    "plugin",
    "TTEIH"
  ],
  "author": "Hunyosi Asakura",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/hunyosi/tteih/issues"
  },
  "homepage": "http://hunyoi.com/pub/tteih/",
  "dependencies": {
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "electron": "^1.4.10",
    "electron-packager": "^8.3.0",
    "iconv-lite": "^0.4.13",
    "jszip": "^3.1.3",
    "mocha": "^3.2.0",
    "source-map-support": "^0.4.0",
    "webpack": "^1.13.1"
  },
  "devDependencies": {
    "npm-check-updates": "^2.8.7"
  }
}
